                        <p><strong>Jawaban:</strong></p><p><strong>D</strong><strong>.</strong><strong> </strong>Lurus hati</p><p><strong>Penjelasan:</strong></p><strong>Dalam bahasa Arab, jujur </strong><strong>bisa</strong><strong> </strong><strong>dikatakan</strong><strong> </strong><strong>as-</strong><strong>shiddiq yang artinya </strong><strong>jujur</strong><strong> </strong><strong>dan</strong><strong> </strong><strong>dapa</strong><strong>t</strong><strong> </strong><strong>dipercaya</strong><strong>.</strong><strong> </strong><strong>Jujur </strong><strong>Berarti</strong><strong> </strong><strong>mengatakan</strong><strong> </strong><strong>hal</strong><strong> </strong><strong>yang</strong><strong> </strong><strong>sesungguhny</strong><strong>.</strong><strong> </strong><strong>Lawan</strong><strong> </strong><strong>Kata</strong><strong> </strong><strong>Jujur</strong><strong> </strong><strong>:</strong><strong> </strong><strong>Bohong</strong><p></p><p><strong>Berikut</strong><strong> </strong><strong>adalah</strong><strong> </strong><strong>orang</strong><strong> </strong><strong>yang</strong><strong> </strong><strong>mendapat</strong><strong> </strong><strong>gelar</strong><strong> </strong><strong>as-shiddiq</strong><strong> </strong><strong>:</strong></p><strong>Abu</strong><strong> </strong><strong>Bakar</strong><strong>,</strong><strong> </strong><strong>Ia</strong><strong> </strong><strong>adalah</strong><strong> </strong><strong>sahabat</strong><strong> </strong><strong>Rasulullah</strong><strong> </strong><strong>yang</strong><strong> </strong><strong>setia</strong><p></p><p></p><strong>ANSWER</strong><strong> </strong><strong>BY</strong><strong> </strong><strong>:</strong><strong> </strong><strong>KANIA</strong><strong> </strong><br />
