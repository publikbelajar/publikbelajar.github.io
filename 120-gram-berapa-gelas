                        <p><strong>M</strong><strong>a</strong><strong>a</strong><strong>f</strong><strong> </strong><strong>j</strong><strong>a</strong><strong>w</strong><strong>a</strong><strong>b</strong><strong>a</strong><strong>n</strong><strong>n</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>a</strong><strong>g</strong><strong>a</strong><strong>k</strong><strong> </strong><strong>b</strong><strong>e</strong><strong>r</strong><strong>t</strong><strong>e</strong><strong>l</strong><strong>e</strong><strong> </strong><strong>t</strong><strong>e</strong><strong>l</strong><strong>e</strong><strong>,</strong><strong> </strong><strong>s</strong><strong>e</strong><strong>b</strong><strong>e</strong><strong>n</strong><strong>a</strong><strong>r</strong><strong>n</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>1</strong><strong> </strong><strong>c</strong><strong>m</strong><strong> </strong><strong>k</strong><strong>u</strong><strong>b</strong><strong>i</strong><strong>k</strong><strong> </strong><strong>i</strong><strong>t</strong><strong>u</strong><strong> </strong><strong>s</strong><strong>a</strong><strong>m</strong><strong>a</strong><strong> </strong><strong>d</strong><strong>e</strong><strong>n</strong><strong>g</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>1</strong><strong> </strong><strong>m</strong><strong>i</strong><strong>l</strong><strong>i</strong><strong>l</strong><strong>i</strong><strong>t</strong><strong>e</strong><strong>r</strong><strong>.</strong><strong> </strong><strong>M</strong><strong>a</strong><strong>a</strong><strong>f</strong><strong>k</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>k</strong><strong>e</strong><strong>s</strong><strong>a</strong><strong>l</strong><strong>a</strong><strong>h</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>s</strong><strong>a</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>y</