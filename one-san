NO NYONTEK NO RIBETT</h2><br />
                        <p><strong>Jawaban:</strong></p><p></p><p><strong>"</strong><strong>N</strong><strong>a</strong><strong>n</strong><strong>i</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>A</strong><strong>p</strong><strong>a</strong></p><p><strong>"</strong><strong>H</strong><strong>a</strong><strong>i</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>I</strong><strong>y</strong><strong>a</strong></p><p><strong>"</strong><strong>W</strong><strong>a</strong><strong>k</strong><strong>a</strong><strong>r</strong><strong>i</strong><strong>m</strong><strong>a</strong><strong>s</strong><strong>t</strong><strong>a</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>S</strong><strong>a</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>m</strong><strong>e</strong><strong>n</strong><strong>g</strong><strong>e</strong><strong>r</strong><strong>t</strong><strong>i</strong><strong> </strong><strong>(</strong><strong>f</strong><strong>o</strong><strong>r</strong><strong>m</strong><strong>a</strong><strong>l</strong><strong>)</strong></p><p><strong>"</strong><strong>W</strong><strong>a</strong><strong>k</strong><strong>k</strong><strong>a</strong><strong>t</strong><strong>a</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>S</strong><strong>a</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>m</strong><strong>e</strong><strong>n</strong><strong>g</strong><strong>e</strong><strong>r</strong><strong>t</strong><strong>i</strong><strong> </strong><strong>(</strong><strong>n</strong><strong>o</strong><strong>n</strong><strong> </strong><strong>f</strong><strong>o</strong><strong>r</strong><strong>m</strong><strong>a</strong><strong>l</strong><strong>)</strong></p><p><strong>"</strong><strong>O</strong><strong>h</strong><strong>a</strong><strong>y</strong><strong>o</strong><strong>u</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>S</strong><strong>e</strong><strong>l</strong><strong>a</strong><strong>m</strong><strong>a</strong><strong>t</strong><strong> </strong><strong>P</strong><strong>a</strong><strong>g</strong><strong>i</strong></p><p><strong>"</strong><strong>I</strong><strong>t</strong><strong>a</strong><strong>d</strong><strong>a</strong><strong>k</strong><strong>i</strong><strong>m</strong><strong>a</strong><strong>s</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>S</strong><strong>e</strong><strong>l</strong><strong>a</strong><strong>m</strong><strong>a</strong><strong>t</strong><strong> </strong><strong>m</strong><strong>a</strong><strong>k</strong><strong>a</strong><strong>n</strong></p><p><strong>"</strong><strong>N</strong><strong>a</strong><strong>n</strong><strong>i</strong><strong>w</strong><strong>o</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>k</strong><strong>a</strong><strong>m</strong><strong>u</strong><strong> </strong><strong>s</strong><strong>i</strong><strong>a</strong><strong>p</strong><strong>a</strong></p><p><strong>"</strong><strong>n</strong><strong>e</strong><strong> </strong><strong>s</strong><strong>a</strong><strong>n</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>K</strong><strong>a</strong><strong>k</strong><strong>a</strong><strong>k</strong><strong> </strong><strong>p</strong><strong>e</strong><strong>r</strong><strong>e</strong><strong>m</strong><strong>p</strong><strong>u</strong><strong>a</strong><strong>n</strong></p><p><strong>"</strong><strong>n</strong><strong>i</strong><strong> </strong><strong>s</strong><strong>a</strong><strong>n</strong><strong>"</strong><strong> </strong><strong>=</strong><strong> </strong><strong>K</strong><strong>a</strong><strong>k</strong><strong>a</strong><strong>k</strong><strong> </strong><strong>L</strong><strong>