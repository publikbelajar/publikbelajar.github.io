><strong>t</strong><strong> </strong><strong>p</strong><strong>a</strong><strong>d</strong><strong>a</strong><strong> </strong><strong>b</strong><strong>a</strong><strong>g</strong><strong>i</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>j</strong><strong>e</strong><strong>n</strong><strong>i</strong><strong>s</strong><strong> </strong><strong>a</strong><strong>t</strong><strong>a</strong><strong>u</strong><strong> </strong><strong>s</strong><strong>p</strong><strong>e</strong><strong>s</strong><strong>i</strong><strong>e</strong><strong>s</strong><strong> </strong><strong>m</strong><strong>a</strong><strong>k</strong><strong>h</strong><strong>l</strong><strong>u</strong><strong>k</strong><strong> </strong><strong>h</strong><strong>i</strong><strong>d</strong><strong>u</strong><strong>p</strong><strong> </strong><strong>d</strong><strong>a</strong><strong>l</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>g</strong><strong>e</strong><strong>n</strong><strong>u</strong><strong>s</strong><strong> </strong><strong>y</strong><strong>g</strong><strong> </strong><strong>s</strong><strong>a</strong><strong>m</strong><strong>a</strong><strong>.</strong><strong> </strong><strong>K</strong><strong>e</strong><strong>a</strong><strong>n</strong><strong>e</strong><strong>r</strong><strong>a</strong><strong>g</strong><strong>a</strong><strong>m</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>j</strong><strong>e</strong><strong>n</strong><strong>i</strong><strong>s</strong><strong> </strong><strong>l</strong><strong>e</strong><strong>b</strong><strong>i</strong><strong>h</strong><strong> </strong><strong>m</strong><strong>u</strong><strong>d</strong><strong>a</strong><strong>h</strong><strong> </strong><strong>d</strong><strong>i</strong><strong> </strong><strong>p</strong><strong>a</strong><strong>h</strong><strong>a</strong><strong>m</strong><strong>i</strong><strong> </strong><strong>d</strong><strong>a</strong><