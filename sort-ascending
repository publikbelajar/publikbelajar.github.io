                                            <h2 class="ez-toc-section" id="array-25787789143959619956-urutkan-data-berikut-dengan-menggunakan-a-metode-straight-selection-secara-ascending-b-metode-bubble-sort-secara-descending-c-metode-merge-sort-secara-ascend">
                            31. Array {25,78,77,89,14,39,59,61,99,56} Urutkan data berikut dengan menggunakan : A. Metode straight selection secara ascending B. Metode bubble sort secara descending C. Metode merge sort secara ascending D. Metode selection sort secara descending</h2><br />
                        <p>A. Straight Selection</p><p>#include &lt;bits/stdc++.h&gt;</p><p>using namespace std; </p><p>  </p><p>void swap(int *xp, int *yp)</p><p>{  </p><p>    int temp = *xp;  </p><p>    *xp = *yp;  </p><p>    *yp = temp;</p><p>}  </p><p>  </p><p>void selectionSort(int arr[], int n)  </p><p>{  </p><p>    int i, j, min_idx;</p><p>  </p><p>    for (i = 0; i &lt; n-1; i++)  </p><p>    {  </p><p>        min_idx = i;</p><p>        for (j = i+1; j &lt; n; j++)  </p><p>        if (arr[j] &lt; arr[min_idx])</p><p>            min_idx = j;  </p><p>  </p><p>        swap(&amp;arr[min_idx], &amp;arr[i]);</p><p>    }  </p><p>}  </p><p>  </p><p>void print(int arr[], int size)       </p><p>{  </p><p>    int i;</p><p>    for (i=0; i &lt; size; i++)  </p><p>        cout &lt;&lt; arr[i] &lt;&lt; " ";</p><p>    cout &lt;&lt; endl;  </p><p>}  </p><p>  </p><p>int main()  </p><p>{  </p><p>    int arr[] = {25,78,77,89,14,39,59,61,99,56} ;      </p><p>    int n = sizeof(arr)/sizeof(arr[0]);  </p><p>    selectionSort(arr, n);</p><p>    cout &lt;&lt; "Array yang telah disort:: \n";</p><p>    print(arr, n);     </p><p>    return 0;  </p><p>}</p><p></p><p>B. Bubble Sort</p><p>#include &lt;bits/stdc++.h&gt;</p><p>using namespace std; </p><p>  </p><p>void bubbleSort(int arr[], int n){</p><p>   int i, j, tmp;</p><p>   for(i=0; i&lt;=n; i++){</p><p>       for(j=i; j&lt;=n; j++){</p><p></p><p>              if(arr[i] &lt; arr[j]){</p><p></p><p>                     tmp = arr[j];</p><p>                     arr[j] = arr[i];</p><p>                     arr[i] = tmp;</p><p>              }</p><p>        }</p><p>    }</p><p>}</p><p>void print(int arr[], int size)     </p><p>{  </p><p>    int i;  </p><p>    for (i = 0; i &lt; size; i++)</p><p>        cout &lt;&lt; arr[i] &lt;&lt; " ";  </p><p>    cout &lt;&lt; endl;</p><p>}  </p><p>  </p><p>int main()  </p><p>{  </p><p>    int arr[] = {25,78,77,89,14,39,59,61,99,56};</p><p>    int n = sizeof(arr)/sizeof(arr[0]);</p><p>    bubbleSort(arr, n);  </p><p>    cout&lt;&lt;"Hasil (Descending): \n";</p><p>    print(arr, n);       </p><p>    return 0;  </p><p>}</p><p></p><p>C. Merge Sort</p><p></p><p>#include &lt;stdio.h&gt; </p><p>#include &lt;stdlib.h&gt; </p><p>  </p><p>void merge(int arr[], int l, int m, int r) </p><p>{ </p><p>    int i, j, k; </p><p>    int n1 = m - l + 1; </p><p>    int n2 = r - m; </p><p>  </p><p>    int L[n1], R[n2]; </p><p>  </p><p>    for (i = 0; i &lt; n1; i++) </p><p>        L[i] = arr[l + i]; </p><p>    for (j = 0; j &lt; n2; j++) </p><p>        R[j] = arr[m + 1 + j]; </p><p>  </p><p>    i = 0; </p><p>    j = 0; </p><p>    k = l;</p><p>    while (i &lt; n1 &amp;&amp; j &lt; n2) { </p><p>        if (L[i] &lt;= R[j]) { </p><p>            arr[k] = L[i]; </p><p>            i++; </p><p>        } </p><p>        else { </p><p>            arr[k] = R[j]; </p><p>            j++; </p><p>        } </p><p>        k++; </p><p>    } </p><p>  </p><p>    while (i &lt; n1) { </p><p>        arr[k] = L[i]; </p><p>        i++; </p><p>        k++; </p><p>    } </p><p>  </p><p>    while (j &lt; n2) { </p><p>        arr[k] = R[j]; </p><p>        j++; </p><p>        k++; </p><p>    } </p><p>} </p><p>  </p><p>void mergeSort(int arr[], int l, int r) </p><p>{ </p><p>    if (l &lt; r) { </p><p>        int m = l + (r - l) / 2; </p><p>  </p><p>        mergeSort(arr, l, m); </p><p>        mergeSort(arr, m + 1, r); </p><p>  </p><p>        merge(arr, l, m, r); </p><p>    } </p><p>} </p><p>  </p><p>void print(int A[], int size) </p><p>{ </p><p>    int i; </p><p>    for (i = 0; i &lt; size; i++) </p><p>        printf("%d ", A[i]); </p><p>    printf("\n"); </p><p>} </p><p>  </p><p>int main() </p><p>{ </p><p>    int arr[] = {25,78,77,89,14,39,59,61,99,56}; </p><p>    int arr_size = sizeof(arr) / sizeof(arr[0]); </p><p>   </p><p>    mergeSort(arr, 0, arr_size - 1); </p><p>  </p><p>    printf("Hasil: \n"); </p><p>    print(arr, arr_size); </p><p>    return 0; </p><p>} </p><p></p><p></p><p></p><br />
