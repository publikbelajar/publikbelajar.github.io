                        <p><strong>R</strong><strong>o</strong><strong>l</strong><strong> </strong><strong>K</strong><strong>e</strong><strong>d</strong><strong>e</strong><strong>p</strong><strong>a</strong><strong>n</strong><strong>.</strong><strong> </strong></p><p></p><p><strong>1</strong><strong>.</strong><strong> </strong><strong>S</strong><strong>i</strong><strong>k</strong><strong>a</strong><strong>p</strong><strong> </strong><strong>p</strong><strong>e</strong><strong>r</strong><strong>t</strong><strong>a</strong><strong>m</strong><strong>a</strong><strong> </strong><strong>j</strong><strong>o</strong><strong>n</strong><strong>g</strong><strong>k</strong><strong>o</strong><strong>k</strong><strong>,</strong><strong> </strong><strong>kedua tangan menumpu pada matras selebar bahu.</strong></p><p><strong>2</strong><strong>.</strong><strong> </strong><strong>S</strong><strong>e</strong><strong>t</strong><strong>e</strong><strong>l</strong><strong>a</strong><strong>h</strong><strong> </strong><strong>i</strong><strong>t</strong><strong>u</strong><strong> </strong><strong>k</strong><strong>e</strong><strong>d</strong><strong>u</strong><strong>a</strong><strong> </strong><strong>k</strong><strong>a</strong><strong>k</strong><strong>i</strong><strong> </strong><strong>d</strong><strong>i</strong><strong> </strong><strong>l</strong><strong>u</strong><strong>r</strong><strong>u</strong><strong>s</strong><strong>k</strong><strong>a</strong><strong>n</strong><strong>,</strong><strong> </strong><strong>siku tangan ditekuk, kepala dilipat sampai dagu menyentuh dada.</strong></p><p><strong>3</strong><strong>.</strong><strong> </strong><strong>k</strong><strong>e</strong><strong>m</strong><strong>u</strong><strong>d</strong><strong>i</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>Mengguling ke depan dengan mendaratkan </strong><strong>t</strong><strong>e</strong><strong>n</strong><strong>g</strong><strong>k</strong><strong>u</strong><strong>k</strong>