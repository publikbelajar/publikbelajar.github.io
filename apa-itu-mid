                                            (* Tampilkan Data *)
     For x:=LB to UB Do Write(Data[x],' ');Writeln;
     Write('Angka yang dicari : ');Readln(Item);   (* Item yg dicari *)
     (* Inisialisasi Awal *)
     Beg := LB; Ending := UB; Mid := (Beg + Ending) DIV 2;
      Write('Beg : ',Beg,' End : ',Ending,' Mid : ',Mid,' Data[',mid,'] : ',Data[Mid]);
      Readln;
(* Proses Perbandingan *)
     JmlBanding :=1;
     While (Beg Item) Do
     Begin
          If Item &lt; Data[Mid] Then 
		Ending := Mid-1 
	    Else 
		Beg := Mid + 1;
          Mid := (Beg + Ending) DIV 2;
          Inc(JmlBanding);
           Write('Beg : ',Beg,' End : ',Ending,' Mid : ',Mid,' Data[',mid,'] : ',Data[Mid]);
          Readln;
