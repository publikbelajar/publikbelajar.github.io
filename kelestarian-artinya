                        <p><strong>Jawaban:</strong></p> <strong>k</strong><strong>e</strong><strong>l</strong><strong>e</strong><strong>s</strong><strong>t</strong><strong>a</strong><strong>r</strong><strong>i</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>a</strong><strong>d</strong><strong>a</strong><strong>l</strong><strong>a</strong><strong>h</strong><strong> </strong><strong>u</strong><strong>p</strong><strong>a</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>u</strong><strong>p</strong><strong>a</strong><strong>y</strong><strong>a</strong><strong> </strong><strong>y</strong><strong>g</strong><strong> </strong><strong>d</strong><strong>a</strong><strong>p</strong><strong>a</strong><strong>t</strong><strong> </strong><strong>d</strong><strong>i</strong><strong>l</strong><strong>a</strong><strong>k</strong><strong>u</strong><strong>k</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>u</strong><strong>n</strong><strong>t</strong><strong>u</strong><strong>k</strong><strong> </strong><strong>m</strong><strong>e</strong><strong>l</strong><strong>e</strong><strong>s</strong><strong>t</strong><strong>a</strong><strong>r</strong><strong>i</strong><strong>k</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>a</strong><strong>l</strong><strong>a</strong><strong>m</strong><strong> </strong><strong>,</strong><strong> </strong><strong>m</strong><strong>e</strong><strong>n</strong><strong>j</strong><strong>a</strong><strong>g</strong><strong>a</strong><strong> </strong><strong>a</strong><strong>l</strong><strong>a</strong><strong>m</strong><strong> </strong><strong>,</strong><strong> </strong><strong>d</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>m</strong><strong>e</strong><strong>l</strong><strong>i</strong><strong>n</strong><strong>d</strong><strong>u</strong><strong>n</strong><strong>g</strong><strong>i</strong><strong> </strong><strong>a</strong><strong>l</strong