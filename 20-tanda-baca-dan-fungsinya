4.
5. If (nilai &gt;= 85){
6. System.out.println("Predikat A");
7. }
8. else if (nilai &gt;= 70){
9. System.out.println("Predikat B");
10. }
11. else {
12. System.out.println ("Predikat C");
13. }
14. }
15. }
