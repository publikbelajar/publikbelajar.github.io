a. K = {hari hari dalam seminggu yang huruf terakhir u}
b. L = {bulan bulan dalam setahun yang lamanya 31 hari}
c. M = {bilangan prima yang kurang dari 20}
d. N = {huruf pembentuk kata "makanan"}
e. P = {huruf pembentuk kata "minuman"}
f. Q = {huruf pembentuk kata "kota indah"}
g. R = {bilangan cacah yang kurang dari 10}
h. T = {bilangan asli yang merupakan faktor dari 12}
i. V = {bilangan bulat antara -3 dan 5}
j. W = {nama nama planet dalam tata surya}
