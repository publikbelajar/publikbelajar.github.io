                        <strong>P</strong><strong>e</strong><strong>n</strong><strong>y</strong><strong>e</strong><strong>l</strong><strong>e</strong><strong>s</strong><strong>a</strong><strong>i</strong><strong>a</strong><strong>n</strong><strong>:</strong><p><strong>Iklan</strong>, informasi yang disampaikan adalah <strong>i</strong><strong>n</strong><strong>f</strong><strong>o</strong><strong>r</strong><strong>m</strong><strong>a</strong><strong>s</strong><strong>i</strong><strong> </strong><strong>t</strong><strong>e</strong><strong>n</strong><strong>t</strong><strong>a</strong><strong>n</strong><strong>g</strong><strong> </strong><strong>b</strong><strong>a</strong><strong>r</strong><strong>a</strong><strong>n</strong><strong>g</strong><strong> </strong><strong>a</strong><strong>t</strong><strong>a</strong><strong>u</strong><strong> </strong><strong>j</strong><strong>a</strong><strong>s</strong><strong>a</strong><strong> </strong><strong>y</strong><strong>a</strong><strong>n</strong><strong>g</strong><strong> </strong><strong>a</strong><strong>k</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>d</strong><strong>i</strong><strong>j</strong><strong>u</strong><strong>a</strong><strong>l</strong><strong> </strong><strong>s</strong><strong>e</strong><strong>c</strong><strong>a</strong><strong>r</strong><strong>a</strong><strong> </strong><strong>u</strong><strong>m</strong><strong>u</strong><strong>m</strong><strong>.</strong></p><p>__________________________________</p><p>Reklame adalah <strong>m</strong><strong>e</strong><strong>d</strong><strong>i</strong><strong>a</strong><strong> </strong><strong>y</strong><strong>a</strong><strong>n</strong><strong>g</strong><strong> </strong><strong>d</strong><strong>i</strong><strong>g</strong><strong>u</strong><strong>n</strong><strong>a</strong><strong>k</strong><strong>a</strong><strong>n</strong><strong> </strong><strong>u</strong><strong>n</strong><strong>t</strong><strong>u</strong><strong>k</stron