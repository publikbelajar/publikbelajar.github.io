            "use strict";
            if ("querySelector" in document && "addEventListener" in window) {
                var e = document.body;
                e.addEventListener("mousedown", function() {
                    e.classList.add("using-mouse")
                }), e.addEventListener("keydown", function() {
                    e.classList.remove("using-mouse")
                })
            }
        }();
    </script>
