                        <p><strong>Jawaban:</strong></p><p></p><strong>C.</strong><strong> </strong><strong>Kumpulan</strong><strong> </strong><strong>hewan</strong><strong> </strong><strong>yang</strong><strong> </strong><strong>berkaki</strong><strong> </strong><strong>empat</strong><strong>A</strong><strong>.</strong><strong> </strong><strong>{</strong><strong>}</strong><strong>D</strong><strong>.</strong><strong> </strong><strong>{</strong><strong>2</strong><strong>,</strong><strong>3</strong><strong>,</strong><strong>4</strong><strong>,</strong><strong>5</strong><strong>,</strong><strong>6</strong><strong>,</strong><strong>7</strong><strong>,</strong><strong>8</strong><strong>}</strong><strong>C</strong><strong>.</strong><strong> </strong><strong>G</strong><strong>=</strong><strong>{</strong><strong>x</strong><strong> </strong><strong>l</strong><strong> </strong><strong>x</strong><strong> </strong><strong>&lt;</strong><strong> </strong><strong>1</strong><strong>5</strong><strong>,</strong><strong> </strong><strong>x</strong><strong> </strong><strong>adalah</strong><strong> </strong><strong>bilangan</strong><strong> </strong><strong>asli</strong><strong>}</strong><strong>D</strong><strong>.</strong><strong> </strong><strong>A</strong><strong>=</strong><strong>{</strong><strong>Himpunan</strong><strong> </strong><strong>bilangan</strong><strong> </strong><strong>ganjil</strong><strong> </strong><strong>antara</strong><strong> </strong><strong>0</strong><strong> </strong><strong>sampai</strong><strong> </strong><strong>1</strong><strong>0</strong><strong>}</strong><strong>B</strong><strong>.</strong><strong> </strong><strong>Himpunan</strong><strong> </strong><strong>nama</strong><strong> </strong><strong>nama</strong><strong> </strong><strong>hari</strong><strong> </strong><strong>yang</strong><strong> </strong><strong>diawali</strong><strong> </strong><strong>dengan</strong><strong> </strong><strong>huruf</strong><strong> </strong><strong>P</strong><strong>C</st